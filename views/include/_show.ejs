<% users.forEach(user =>{ %>
<tr>

  <td><%= user.name %></td>
  <td><%= user.email %></td>
  <td><%= user.gender %></td>
  <td><%= user.status %></td>
  <td>
    <a href="/update-user?id=<%= user._id %>" class="btn border-shadow update">
      <span class="text-gradient">
        <i class="fas fa-pencil-alt"></i>
      </span>
    </a>
    <a data-id="<%= user._id %>" class="btn border-shadow delete ">
      <span class=" " style="color: rgb(255, 0, 0);">
        <i class="fas fa-trash"></i>
      </span>
    </a>
  </td>
</tr>
<% }) %>
